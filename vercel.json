{
    "version": 2,
    "builds": [
        {
            "src": "api/server.ts",
            "use": "@vercel/node"
        }
    ],
    "routes": [
        {
            "src": "/",
            "dest": "public/index.html"
        },
        {
            "src": "/api/(.*)",
            "dest": "api/server.ts"
        }
    ]
}