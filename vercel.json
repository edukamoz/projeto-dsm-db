{
    "version": 2,
    "builds": [
        {
            "src": "dist/src/server.ts",
            "use": "@vercel/node"
        }
    ],
    "routes": [
        {
            "src": "/",
            "dest": "public/index.html"
        },
        {
            "src": "/(.*\\.(js|css|html|png|jpg|jpeg|svg|ico))",
            "dest": "public/$1"
        },
        {
            "src": "/(.*)",
            "dest": "dist/src/server.ts"
        }
    ]
}