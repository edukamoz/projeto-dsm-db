{
    "version": 2,
    "env": {
        "MONGODB_URI": "@mongodb_uri",
        "API_URL": "http://localhost:3000/api/books"
    },
    "builds": [
        {
            "src": "src/server.ts",
            "use": "@vercel/node"
        }
    ],
    "routes": [
        {
            "src": "/api/(.*)",
            "dest": "/src/server.ts"
        },
        {
            "src": "/(.*)",
            "dest": "/public/index.html"
        }
    ]
}